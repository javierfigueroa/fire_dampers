<div class="row-fluid">
	<%= render 'navlist' %>
	<div class="span10">
		<div class="row">
			<div class="span10">
				<ul class="breadcrumb">
					<li>
						<a href="#">Home</a><span class="divider">/</span>
					</li>
					<li class="active">
						<a href="#">Users</a>
					</li>
				</ul>
			</div>
		</div>
		<div class="row">
			<div class="span10">
				<div class="page-header">
					<h1>Users</h1>
				</div>
				<div class="row">
					<div class="span8">
						<ul class="nav nav-pills">
							<li>
								<a href="<%= new_user_path %>"><i class="icon-file"></i> New User</a>
							</li>
						</ul>
					</div>
				</div>
			</div>
		</div>
		<div class="row">
			<div class="span10">
				<div class="container data_table" style="margin-top: 10px">
					<div id="example_wrapper" class="dataTables_wrapper form-inline" role="grid">
						<table cellspacing="0" cellpadding="0" border="0" id="example" class="table table-striped table-bordered dataTable" aria-describedby="example_info">
							<thead>
								<tr role="row">
									<!-- <th class="sorting_asc" role="columnheader" tabindex="0" aria-controls="example" rowspan="1" colspan="1" style="width: 161px;" aria-sort="ascending" aria-label="Rendering engine: activate to sort column descending">Rendering engine</th><th class="sorting" role="columnheader" tabindex="0" aria-controls="example" rowspan="1" colspan="1" style="width: 236px;" aria-label="Browser: activate to sort column ascending">Browser</th><th class="sorting" role="columnheader" tabindex="0" aria-controls="example" rowspan="1" colspan="1" style="width: 221px;" aria-label="Platform(s): activate to sort column ascending">Platform(s)</th><th class="sorting" role="columnheader" tabindex="0" aria-controls="example" rowspan="1" colspan="1" style="width: 136px;" aria-label="Engine version: activate to sort column ascending">Engine version</th><th class="sorting" role="columnheader" tabindex="0" aria-controls="example" rowspan="1" colspan="1" style="width: 100px;" aria-label="CSS grade: activate to sort column ascending">CSS grade</th>
									-->
									<th class="actions">Actions</th>
									<th>Status</th>
									<th>First name</th>
									<th>Last name</th>
									<th>Role</th>
									<th>Email</th>
									<th>Registration date</th>
									<th>Last login date</th>
								</tr>
							</thead>
							<tbody role="alert" aria-live="polite" aria-relevant="all">
								<% @users.each do |user| %>
								<tr>
									<td><a href="<%= url_for(user) %>"><i class="icon-eye-open"></i></a><a href="<%= edit_user_path(user) %>"><i class="icon-cog"></i></a><a href="<%= url_for(user) %>" rel="nofollow" data-method="delete"
									data-confirm="Are you sure?"><i class="icon-trash"></i></a></td>
									<td><% if user.active? %>
					ACTIVE
					<% else %>
					DISABLED
					<%end%></td>
									<td><%= user.first_name %></td>
									<td><%= user.last_name %></td>
									<td><%= user.role %></td>
									<td><%= user.email %></td>
									<td><%= user.created_at %></td>
									<td><%= user.last_sign_in_at %></td>
								</tr>
								<% end %>
							</tbody>
						</table>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
</div> 